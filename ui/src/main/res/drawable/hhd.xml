    <!--背景-->
    <item
        android:width="20dp"
        android:height="10dp">
        <shape android:shape="rectangle">
            <solid android:color="#fff2f2f2" />
            <corners
                android:bottomLeftRadius="0dp"
                android:bottomRightRadius="0dp"
                android:topLeftRadius="2dp"
                android:topRightRadius="2dp" />
        </shape>
    </item>
    <!--线-->
    <item
        android:width="1dp"
        android:height="7dp"
        android:left="0dp"
        android:top="2dp">
        <rotate
            android:fromDegrees="0.0"
            android:pivotX="0%"
            android:pivotY="0%"
            android:toDegrees="0.0">
            <shape>
                <gradient
                    android:angle="270"
                    android:endColor="#3333"
                    android:startColor="#3333"
                    android:type="linear"
                    android:useLevel="false" />
            </shape>
        </rotate>
    </item>

    <item
        android:width="1dp"
        android:height="8dp"
        android:left="19dp"
